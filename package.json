{
  "name": "serverless-auction",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "test": "jest --watchAll --colors",
    "test:ci": "jest --ci --colors --coverage --passWithNoTests",
    "lint": "eslint -c .eslintrc.js --ext .ts .",
    "deploy": "sls deploy -v",
    "deploy-fn": "sls deploy function -f $FUNCTION",
    "mock-fn": "sls invoke -f $FUNCTION -p src/mocks/$FUNCTION-event.json -l",
    "mock-fn-local": "sls invoke local -f $FUNCTION -p src/mocks/$FUNCTION-event.json",
    "tail-fn": "sls logs -f $FUNCTION -t"
  },
  "dependencies": {
    "@middy/core": "^1.4.0",
    "@middy/http-error-handler": "^1.4.0",
    "@middy/http-json-body-parser": "^1.4.0",
    "@middy/validator": "^1.4.0",
    "@types/aws-lambda": "^8.10.63",
    "@types/http-errors": "^1.8.0",
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.740.0",
    "http-errors": "^1.8.0",
    "serverless-pseudo-parameters": "^2.5.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^10.12.18",
    "@types/serverless": "^1.72.5",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "fork-ts-checker-webpack-plugin": "^3.0.1",
    "jest": "^26.5.3",
    "serverless-webpack": "^5.2.0",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.2.4",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
